<UserControl
    x:Class="RS_StandardComponents.MsgBoxContent"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:RS_StandardComponents"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    
    mc:Ignorable="d">
    <Grid>
        <StackPanel>
            <ContentPresenter x:Name="ContentPresenter"></ContentPresenter>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Bottom">
            <Button Content="OK" Margin="10" />
            <Button Content="Cancel"  Margin="10"/>
        </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
